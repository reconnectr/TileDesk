  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.1",
    "sinon": "^7.5.0",
    "sinon-mongoose": "^2.3.0",
    "mongodb-runner": "4.7.5",
    "loadtest": "*"
  }
