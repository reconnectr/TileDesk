{
  "name": "@tiledesk/tiledesk-server",
  "description": "The Tiledesk server module",
  "version": "2.1.0-beta.6",
  "scripts": {
    "start": "node ./bin/www",
    "pretest": "mongodb-runner start",
    "watchenv": "nodemon --watch .env --watch ./confenv/.env ./bin/www",
    "test": "mocha ./test/*.js ./pubmodules/**/test/*.js ./modules/**/test/*.js ./channels/*/test/*.js ./node_modules/@tiledesk-ent/**/test/*.js  --exit",
    "test:int": "mocha ./test-int/*.js  ./pubmodules/**/test-int/*.js ./modules/**/test-int/*.js ./channels/**/test-int/*.js --exit",
    "posttest": "mongodb-runner stop",
    "preinstall": "if [ $ENABLE_ENTERPRISE_MODULE ]; then npm run enable-ent; fi",
    "enable-ent": "echo //registry.npmjs.org/:_authToken=${NPM_TOKEN} > .npmrc"
  },
  "private": false,
  "author": "Andrea Leo - Frontiere21 SRL",
  "license": "AGPL-3.0",
  "homepage": "https://www.tiledesk.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tiledesk/tiledesk-server"
  },
  "engines": {
    "node": "11.10.0",
    "npm": "6.8.0"
  },
  "bin": {
    "tiledesk-server": "./bin/www"
  },
  "optionalDependencies": {
    "@tiledesk-ent/tiledesk-server-payments": "^1.1.3",
    "@tiledesk-ent/tiledesk-server-analytics": "^1.1.5",
    "@tiledesk-ent/tiledesk-server-activities": "^1.1.15",
    "@tiledesk-ent/tiledesk-server-resthook": "^1.1.42",
    "@tiledesk-ent/tiledesk-server-triggers": "^1.1.61",
    "@tiledesk-ent/tiledesk-server-jwthistory": "^1.1.7",
    "@tiledesk-ent/tiledesk-server-dialogflow": "^1.1.6",
    "@tiledesk-ent/tiledesk-server-request-history": "^1.1.5",
    "@tiledesk-ent/tiledesk-server-routing-queue": "^1.1.9",
    "@tiledesk-ent/tiledesk-server-queue": "^1.1.9",
    "@tiledesk-ent/tiledesk-server-cache": "^1.1.8"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.3.1",
    "@tiledesk/tiledesk-chat21-app": "^1.0.14",
    "apollo-server": "^2.9.12",
    "apollo-server-express": "^2.9.12",
    "app-root-path": "^3.0.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "~1.4.4",
