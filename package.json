    "csv-express": "^1.2.2",
    "debug": "~4.1.1",
    "dotenv": "^8.2.0",
    "email-templates": "^6.0.4",
    "eventemitter2": "^5.0.1",
    "express": "~4.17.1",
    "express-session": "^1.17.0",
    "express-validator": "^6.3.1",
    "fast-csv": "^3.4.0",
    "get-timezone-offset": "^1.0.3",
    "graphql": "^14.5.8",
